# 数据库迁移工具环境变量配置示例
# 复制此文件到导入脚本同目录下并重命名为.env

# MySQL数据库配置
MYSQL_HOST=localhost
MYSQL_USER=root
MYSQL_PASSWORD=your_password
MYSQL_DATABASE=billiards_app
MYSQL_PORT=3306

# MongoDB数据库配置
MONGODB_URI=mongodb://localhost:27017/billiards_app

# 加密配置
SALT_ROUNDS=10

# JWT配置（如果您的新后端使用JWT认证）
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d 